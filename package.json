{
  "name": "tindhair",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "~18.0.0",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "build-android": "cd android && ./gradlew assembleRelease && cd .. && cp android/app/build/outputs/apk/app-release.apk builds/release.apk",
    "build-ios": "react-native run-ios --configuration Release && cd ios/build/Build/Products/Release-iphonesimulator/ && rm -rf Payload && mkdir Payload && cp -r tindhair.app Payload/tindhair.app && zip -r Payload.zip Payload &&  cd ../../../../.. && cp ios/build/Build/Products/Release-iphonesimulator/Payload.zip builds/release.ipa",
    "start-emulator": "source ~/.bashrc && emulator -avd Nexus",
    "ios": "react-native run-ios",
    "ios-log": "react-native log-ios",
    "test": "node node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "react": "16.0.0-alpha.12",
    "react-mixin": "^2.0.2",
    "react-native": "^0.47.1",
    "react-native-busy-indicator": "^1.1.0",
    "react-native-fs": "^2.5.1",
    "react-native-image-picker": "^0.26.4",
    "react-native-linear-gradient": "^2.2.0",
    "react-native-message-bar": "^1.6.0",
    "react-native-slider": "^0.11.0",
    "react-native-swipe-cards": "^0.1.0",
    "react-native-swipeable": "^0.6.0",
    "react-native-triangle": "0.0.8",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
